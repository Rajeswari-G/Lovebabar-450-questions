#include <stdio.h>

int main()
{
    int a[10],i,n;
    printf("enter the total no.of array elements:");
    scanf("%d",&n);
    printf("enter the array elements:");
    for(i=0;i<n;i++)
    {
    scanf("%d",&a[i]);
    }
    printf("\nreversed array :");
    for(i=n-1;i>=0;i--)
    {
        printf("%d ",a[i]);
    }

    return 0;
}
